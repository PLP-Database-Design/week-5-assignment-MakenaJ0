<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Providers List</title>
    </head>
    <body>
        <table>
            <thead>
                <tr>
                    
                    <th>Patient ID</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Date of Birth</th>

                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Provider Specialty</th>
                    <th>Email Address</th>

                    <th>Patients First Name</th>

                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Provider Specialty</th>
                </tr>
            </thead>
            <tbody>
                <% results.forEach(row=> {%>
                <tr>
                    <td><%= row.patient_id %></td>
                    <td><%= row.first_name %></td>
                    <td><%= row.last_name %></td>
                    <td><%= row.date_of_birth %></td>

                    <td><%= row.first_name %></td>
                    <td><%= row.last_name %></td>
                    <td><%= row.provider_specialty %></td>
                    <td><%= row.email_address %></td>

                    <td><%= row.first_name %></td>

                    <td><%= row.first_name %></td>
                    <td><%= row.last_name %></td>
                    <td><%= row.provider_specialty %></td>
                </tr>

                <%}); %>
            </tbody>
        </table>
    </body>
</html>
